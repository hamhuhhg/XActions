    "twitter-tools",
    "social-media-automation",
    "twitter-api-alternative",
    "puppeteer",
    "twitter-manager",
    "follower-management",
    "twitter-cleanup",
    "auto-follow",
    "auto-like",
    "twitter-analytics",
    "mcp",
    "model-context-protocol",
    "ai-agent",
    "claude",
    "saas"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
